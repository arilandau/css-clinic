  <h1 class="main-title">Welcome to the Disney League!</h1>


  <div class="row medium-up-2 large-up-4">
      <% @teams.each do |team, players| %>
      <div class="column column-block">

        <a class="index" href="/teams/<%= team %>"><%= team %></a>
        <img src="<%= players[:Thumbnail] %>" class="thumbnail" alt="">

      </div>
      <% end %>
  </div>
